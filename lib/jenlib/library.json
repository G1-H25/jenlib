{
  "name": "jenlib",
  "version": "1.0.0",
  "description": "Jennifer's embedded library for GPIO, BLE, time, and state management",
  "keywords": ["embedded", "gpio", "ble", "arduino", "sensors"],
  "repository": {
    "type": "git",
    "url": "https://github.com/G1-H25/jenlib.git"
  },
  "authors": [
    {
      "name": "Jennifer Gott",
      "email": "jennifer.gott@chasacademy.se"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/G1-H25/jenlib",
  "dependencies": {},
  "frameworks": ["arduino"],
  "platforms": ["espressif32", "espressif8266", "atmelsam", "teensy", "ststm32"],
  "srcFilter": [
    "+<*>",
    "-<src/ble/drivers/NativeBleDriver.cpp>",
    "-<src/ble/drivers/NativeBleService.cpp>",
    "-<src/ble/drivers/NativeBleCharacteristic.cpp>",
    "-<src/time/drivers/NativeTimeDriver.cpp>",
    "-<src/gpio/drivers/NativeGpioDriver.cpp>",
    "-<src/onewire/drivers/NativeOneWireBus.cpp>",
    "-<tests/>",
    "-<smoke_tests/>",
    "-<examples/>"
  ],
  "build": {
    "flags": [
      "-std=gnu++17",
      "-fexceptions"
    ]
  }
}
